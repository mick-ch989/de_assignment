  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": [
      {
        "name": "instance",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(spark_master_alive_workers, instance)",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "links": []
