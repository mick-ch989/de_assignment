{
  "uid": "spark-monitoring",
  "title": "Spark Cluster Monitoring",
  "tags": ["spark", "monitoring", "cluster"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Spark Master - Alive Workers",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "up{job=\"spark-master\"}",
          "legendFormat": "Exporter Status",
          "refId": "A"
        },
        {
          "expr": "spark_master_alive_workers",
          "legendFormat": "Alive Workers",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 1, "color": "yellow"},
              {"value": 2, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Spark Master - Active Applications",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "targets": [
        {
          "expr": "spark_master_alive_applications",
          "legendFormat": "Active Apps",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "Spark Master - Waiting Applications",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "spark_master_waiting_applications",
          "legendFormat": "Waiting Apps",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 2, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Spark Master - Cores Usage",
      "type": "gauge",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
      "targets": [
        {
          "expr": "spark_master_cores_used",
          "legendFormat": "Used",
          "refId": "A"
        },
        {
          "expr": "spark_master_cores_max",
          "legendFormat": "Max",
          "refId": "B"
        }
      ],
      "options": {
        "min": 0,
        "max": 4,
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"value": 0, "color": "green"},
            {"value": 2, "color": "yellow"},
            {"value": 3, "color": "red"}
          ]
        }
      }
    },
    {
      "id": 5,
      "title": "Spark Master - Memory Usage",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "targets": [
        {
          "expr": "spark_master_memory_used_bytes / 1024 / 1024",
          "legendFormat": "Memory Used (MB) - {{instance}}",
          "refId": "A"
        },
        {
          "expr": "spark_master_memory_max_bytes / 1024 / 1024",
          "legendFormat": "Memory Max (MB) - {{instance}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "mbytes"
        }
      }
    },
    {
      "id": 6,
      "title": "Spark Master - Cores Usage Over Time",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "targets": [
        {
          "expr": "spark_master_cores_used",
          "legendFormat": "Cores Used - {{instance}}",
          "refId": "A"
        },
        {
          "expr": "spark_master_cores_max",
          "legendFormat": "Cores Max - {{instance}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
